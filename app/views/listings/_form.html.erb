<div class="container mt-3">
  <%= simple_form_for listing do |f| %>
  <%= f.input :title %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :negotiable %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit "Submit listing", class: "btn btn-info" %>
  <% end %>
</div>
